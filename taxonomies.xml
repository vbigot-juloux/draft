                            <category n="22" xml:id="satisfaction"/>
                            <category n="23" xml:id="shame"/>
                            <category n="25" xml:id="stress"/>
                            <category n="26" xml:id="surprise"/>
                            <category n="27" xml:id="unsatisfaction"/>
                            <category n="28" xml:id="unknown_E"/>
                        </category>
                         <category xml:id="hasEmotionIntensity" ana="EmotionCategory">
                                <category n="1" xml:id="feeble"/>
